generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Alert {
  id        Int      @id @default(autoincrement())
  userId    Int      
  city      String
  message   String   
  createdAt DateTime @default(now())
}