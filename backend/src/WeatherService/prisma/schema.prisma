datasource db {
  provider = "postgresql" // Opções: "postgresql", "mysql", "sqlite", "sqlserver", "mongodb"
  
}

generator client {
  provider = "prisma-client-js"
}

model Weather {
  id        Int      @id @default(autoincrement())
  city      String
  temp      Float?
  humidity  Int?
  wind      Float?
  condition String?
  createdAt DateTime @default(now())
}
